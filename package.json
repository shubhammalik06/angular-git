{
  "name": "material-design",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint:js": "eslint . --ext .js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "test:related": "jest --bail --findRelatedTests"
